{
  "name": "lomment-cine",
  "displayName": "Lomment-Cine",
  "description": "Store unused code instead of commenting them out",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lomment-cine.storeComment",
        "title": "Store Unused Code",
        "icon": "$(archive)"
      },
      {
        "command": "lomment-cine.showStoredCode",
        "title": "View Stored Code"
      },
      {
        "command": "lomment-cine.restoreCode",
        "title": "Restore Code"
      },
      {
        "command": "lomment-cine.deleteStoredCode",
        "title": "Delete Stored Code"
      },
      {
        "command": "lomment-cine.searchCode",
        "title": "Lomment: Search Stored Code",
        "icon": "$(search)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "lomment-cine.storeComment",
          "group": "navigation@1"
        }
      ],
      "editor/title": [
        {
          "when": "editorHasSelection",
          "command": "lomment-cine.storeComment",
          "group": "navigation"
        },
        {
          "command": "lomment-cine.searchCode",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npx tsc -p ./",
    "compile": "npx tsc -p ./",
    "watch": "npx tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
